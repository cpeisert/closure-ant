{namespace tasks2}

/**
 * This template will be rendered by SoyWeb when the user loads tasks2.soy.
 * This makes it possible to see how an empty task list will be rendered.
 */
{template .soyweb}
{call tasks.demoPage}
  {param contentHtml}
    {call tasks.list}
      {param title: 'Food Shopping' /}
      {param items: [] /}
    {/call}
  {/param}
{/call}
{/template}
